    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "update-http-statuses": "esno scripts/update-http-statuses.ts"
